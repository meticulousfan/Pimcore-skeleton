{% apply spaceless %}
<html lang="{{ app.request.locale|replace({ '_': '-' }) }}">
<head>
    {% include 'Layout/html-head.html.twig' %}
    {% block metadata %}
        {% if pimcore_website_config('name') is not empty %}
            {% do pimcore_head_title().append(pimcore_website_config('name')) %}
            {% do pimcore_head_title().setSeparator(' | ') %}
        {% endif %}

        {{ pimcore_head_title() }}
        {{ pimcore_head_meta() }}
    {% endblock %}
    {% block styles %}
        {{ encore_entry_link_tags('css/global') }}
        {% if editmode %}{{ encore_entry_link_tags('css/editmode') }}{% endif %}
    {% endblock %}
    {% block scripts %}
        {{ encore_entry_script_tags('js/app') }}
    {% endblock %}
    {% block headscripts deferred %}
        {{ pimcore_head_script() }}
    {% endblock %}
</head>
<body>
{% include 'Layout/header.html.twig' %}
<main data-uk-height-viewport="expand: true">
    {% block content %}{% endblock %}
</main>
{% include 'Layout/footer.html.twig' %}
</body>
</html>
{% endapply %}
