{% set navStartNode = document.getProperty('nav_start_node')|default(pimcore_document(1)) %}
{% set navigation = pimcore_build_nav({ active: document, root: navStartNode }) %}
{% set menuRenderer = pimcore_nav_renderer('menu') %}

<div class="uk-sticky" data-uk-sticky="sel-target: .uk-navbar-container; cls-active: uk-navbar-sticky">
    <nav class="uk-navbar-container">
        <div class="uk-container uk-container-expand">
            <div class="uk-navbar" data-uk-navbar>

                <div class="uk-navbar-left">
                    <a class="uk-navbar-item uk-logo" href="{{ navStartNode }}">
                        <img width="200" height="50" src="{{ asset('build/images/logo-claim.svg') }}" alt="{{ pimcore_website_config('name') }}">
                    </a>

                    <div class="uk-visible@s">
                        {{ include('navigation/navbar.html.twig') }}
                    </div>
                </div>

                <div class="uk-navbar-right">
                    <div class="uk-hidden@s">
                        <a class="uk-navbar-toggle" href="#mobile-navigation" data-uk-toggle>
                            <span class="uk-icon uk-navbar-toggle-icon" data-uk-navbar-toggle-icon></span>
                        </a>

                        {{ include('navigation/mobile-navigation.html.twig') }}
                    </div>
                </div>

            </div>
        </div>
    </nav>
</div>

{{ include('layout/flash-messages.html.twig') }}
